   "source": [
    "def print_character_grid(word):\n",
    "    length = len(word)\n",
    "    for i in range(1, length + 1):\n",
    "        # Print first i characters of the word in a row\n",
    "        print(word[:i])\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    input_word = input(\"Enter a word or string: \")\n",
    "    print_character_grid(input_word)\n"
   ]
